{
  "name": "@smart-ui/icons",
  "packageManager": "yarn@1.22.17",
  "version": "0.1.0",
  "description": "Smart UI ICONS Library.",
  "author": "South Smart",
  "private": true,
  "workspaces": [
    "src/*"
  ],
  "engines": {
    "node": ">= 16"
  },
  "repository": {
    "type": "git",
    "url": "http://172.16.11.133/front-end/smartui/icons.git"
  },
  "scripts": {
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx --max-warnings 0 --fix",
    "format": "prettier --write .",
