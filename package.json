  },
  "peerDependencies": {
    "vue": ">=3.2.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@smart/eslint-config-typescript": "^1.1.0",
    "eslint": "^7.32.0",
    "eslint-define-config": "^1.2.0",
    "prettier": "^2.5.1",
    "standard-version": "^9.3.2",
    "typescript": "4.5.4"
  },
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all"
  ]
}
